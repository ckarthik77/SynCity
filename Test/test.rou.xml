<?xml version="1.0" encoding="UTF-8"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <!-- Vehicle type -->
    <vType id="car" accel="2.0" decel="4.5" sigma="0.5"
           length="5.0" maxSpeed="15" color="red"/>

    <!-- Routes -->
    <route id="r1" edges="north_in south_out"/>
    <route id="r2" edges="south_in north_out"/>
    <route id="r3" edges="east_in west_out"/>
    <route id="r4" edges="west_in east_out"/>
    <route id="r5" edges="north_in east_out"/>
    <route id="r6" edges="north_in west_out"/>
    <route id="r7" edges="south_in east_out"/>
    <route id="r8" edges="south_in west_out"/>
    <route id="r9" edges="east_in north_out"/>
    <route id="r10" edges="east_in south_out"/>
    <route id="r11" edges="west_in north_out"/>
    <route id="r12" edges="west_in south_out"/>

    <!-- Vehicle flows -->
    <flow id="f1" type="car" route="r1" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f2" type="car" route="r2" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f3" type="car" route="r3" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f4" type="car" route="r4" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="f5" type="car" route="r5" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f6" type="car" route="r6" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f7" type="car" route="r7" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f8" type="car" route="r8" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f9" type="car" route="r9" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f10" type="car" route="r10" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f11" type="car" route="r11" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="f12" type="car" route="r12" begin="0" end="3600" vehsPerHour="200"/>

</routes>
